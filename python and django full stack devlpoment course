#python level one 
part one number

my_income = 100
tax_rate = 0.1
my_taxes = my_income * tax_rate 
print(my_taxes)

part two strings 

mystring = 'abcdefg'
print(mystring[2:])

mystring = 'abcdefg'
print(mystring[::1])

mystring = 'abcdefg'
x = mystring.upper()
print(x)

mystring = 'abcdefg'
x = mystring.capitalize()
print(x)

mystring = 'Hello World'
x = mystring.split('o')
print(x)

x = "Item One: {x} Item Two: {y}".format(x ="dog", y="cat")
print(x)

part three Lists
mylist = ['a','b','c','d','e']
print(mylist[1:])

mylist = ['a','b','c','d','e']
print('before reassignment:')
print(mylist)
mylist[0] = 'NEW ITEM'
print("after reassignment:"
print(mylist)

mylist = ['a','b','c','d','e']
mylist.append(["x",'y','z'])
print(mylist)

mylist =['a','b','c','d','e']
listtwo = ["x",'y','z']
mylist.extend(listtwo)
print(mylist)

mylist =['a','b','c','d','e']
item = mylist.pop()
print(mylist)
print(item)

mylist =['a','b','c','d','e']
mylist.reverse()
print(mylist)

mylist = [4,2,6,1,,43]
mylist.sort()
print(mylist)

mylist = [1,2,['x','y','z']]
print(mylist[2][1])

matrix = [[1,2,3],[4,5,6],[7,8,9]]
first_col = [row[0] for row in matrix]
print(first_col)

part four Dictionaries
my_stuff = {'key1":123,'key2':'value2','key3':{'123':[1,2,3]})
print(my_stuff)

my_stuff = {'key1":123,'key2':'value2','key3':{'123':[1,2,'grabme']})
print(my_stuff['key3']['123'][2].upper())

my_stuff = {'lunch':'pizza','bfast':'eggs'}
my_stuff['lunch'] = 'burger'
my_stuff['dinner'] = 'pasta'
print(my_stuff['lunch'])
print(my_stuff)

